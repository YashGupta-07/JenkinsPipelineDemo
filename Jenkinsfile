pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
    
        stage('Build') {
            steps {
                echo 'Building here'
            }
        }
    
    
    
        stage('Deploy') {
            steps {
                echo 'Deploying here'
            }
        }
    
    
    
   
        stage('Test') {
            steps {
                echo 'Testing here'
            }
        }
        
        
        
        stage('Release') {
            steps {
                echo 'Releasing here'
            }
        }
    }
}
